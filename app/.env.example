# Solana Configuration
NEXT_PUBLIC_SOLANA_NETWORK=devnet
NEXT_PUBLIC_RPC_URL=https://api.devnet.solana.com

# $FBSD Token (TODO: Replace after launch)
# For devnet testing, can use mock mint or leave as placeholder
NEXT_PUBLIC_FBSD_TOKEN_MINT=FBSD_TOKEN_MINT_PLACEHOLDER

# App Wallet (for receiving fees)
NEXT_PUBLIC_APP_WALLET=YOUR_APP_WALLET_ADDRESS

# Dev Wallet (for simulating token buys - optional, for testing only)
# Generate with: solana-keygen new -o dev-wallet.json
# Then: cat dev-wallet.json | jq -c '.[:32]' (first 32 bytes as array)
NEXT_PUBLIC_DEV_WALLET_SECRET=

# Supabase (optional - can use API routes fallback)
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Encryption Key (change in production!)
NEXT_PUBLIC_ENCRYPTION_KEY=default-key-change-in-production

# NFT.Storage (for IPFS image uploads)
NEXT_PUBLIC_NFT_STORAGE_KEY=
